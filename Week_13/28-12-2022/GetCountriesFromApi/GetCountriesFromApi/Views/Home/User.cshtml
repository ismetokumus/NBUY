@model List<UserViewModel>

<div class="container">
    <div class="row">
        @foreach (var user in Model)
        {
            <div class="col-4">
                <div class="card" style="width: 18rem;">
                    <img src="@user.avatar" class="card-img-top" alt="...">
                    <div class="card-body">
                        <h5 class="card-title">@user.first_name</h5>
                        <p class="card-text">@user.email</p>

                    </div>
                </div>
            </div>
        }

    </div>
</div>